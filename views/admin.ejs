            border-radius: 4px;
            position: fixed;
            bottom: 30px;
            right: 30px;
            transition: background-color .1s;
            z-index: 1000;
        }

        #backButton::after {
            content: "\f077";
            font-family: FontAwesome;
            font-weight: normal;
            font-style: normal;
            font-size: 2em;
            line-height: 50px;
            color: #fff;
        }

        #backButton:hover {
            cursor: pointer;
            background-color: #333;
        }

        #backButton:active {
            background-color: #555;
        }
